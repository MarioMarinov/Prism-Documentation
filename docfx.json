{

  "build": {
    "markdownEngineName": "dfm-latest",
    "content": [
      {
        "files": [
          "**/*.md",
          "**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "docs/getting-started/images/**",
          "docs/platforms/maui/images/**",
          "docs/platforms/wpf/images/**",
          "docs/platforms/xamarin-forms/images/**"
        ],
        "exclude": [
          ".github/**",
          ".vscode/**",
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["templates/prism"],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "globalMetadata": {
      "_appTitle": "Prism",
      "_appFooter": "Copyright 2015-2024 Prism Software, LLC",
      "_appLogoPath": "images/prism-logo.png",
      "_gitContribute": {
        "repo": "https://github.com/PrismLibrary/Prism-Documentation",
        "branch": "master"
      }
    }
  }
}